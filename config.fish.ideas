# don't dhow fish greeting
set fish_greeting ""

# ?
set -x CLICOLOR 1

# set default browser action
set -x BROWSER open

# go to home dir
cd ~

# support for user host platform specific commands for your dotfiles repo
. ~/.config/fish/fish-inwater/specifics.fish

# default java location
set -x JAVA_HOME "/System/Library/Frameworks/JavaVM.framework/Versions/1.6/Home"

